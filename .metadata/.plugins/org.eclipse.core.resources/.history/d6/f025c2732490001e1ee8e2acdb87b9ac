import java.io.File;
import java.io.FileNotFoundException;
import java.util.*;

public class Day1 {

	public static void main(String[] args) throws FileNotFoundException {
		Scanner s = new Scanner(new File("data"));
		String line;
		int sum = 0;
		while(s.hasNext()) {
			line = s.nextLine();
			ArrayList<Integer> list = new ArrayList<Integer>();
			for (int i = 0; i<line.length(); i++) {
				if (Character.isDigit(line.charAt(i))) {
					list.add(Character.getNumericValue(line.charAt(i)));
				}
				if (line.charAt(i) == 'o') {
					if (line.charAt(i+1) == 'n') {
						if (line.charAt(i+2) == 'e') {
							list.add(1);
						}
					}
				}
				if (line.charAt(i) == 't') {
					if (i < line.length() && line.charAt(i+1) == 'w') {
						if (i < line.length() && line.charAt(i+2) == 'o') {
							list.add(2);
						}
					}
					if (i < line.length() && line.charAt(i+1) == 'h') {
						if (i < line.length() && line.charAt(i+2) == 'r') {
							if (i < line.length() && line.charAt(i+3) == 'e') {
								if (i < line.length() && line.charAt(i+4) == 'e') {
									list.add(3);
								}
							}
						}
					}
				}
				if (i < line.length() && line.charAt(i) == 'f') {
					if (i < line.length() && line.charAt(i+1) == 'o') {
						if (i < line.length() && line.charAt(i+2) == 'u') {
							if (i < line.length() && line.charAt(i+3) == 'r') {
								list.add(4);
							}
						}
					}
					if (i < line.length() && line.charAt(i+1) == 'i') {
						if (i < line.length() && line.charAt(i+2) == 'v') {
							if (i < line.length() && line.charAt(i+3) == 'e') {
								list.add(5);
							}
						}
					}
				}
				if (i < line.length() && line.charAt(i) == 's') {
					if (i < line.length() && line.charAt(i+1) == 'i') {
						if (i < line.length() && line.charAt(i+2) == 'x') {
							list.add(6);
						}
					}
					if (i < line.length() && line.charAt(i+1) == 'e') {
						if (i < line.length() && line.charAt(i+2) == 'v') {
							if (i < line.length() && line.charAt(i+3) == 'e') {
								if (i < line.length() && line.charAt(i+4) == 'n') {
									list.add(7);
								}
							}
						}
					}
				}
				if (i < line.length() && line.charAt(i) == 'e') {
					if (i < line.length() && line.charAt(i+1) == 'i') {
						if (i < line.length() && line.charAt(i+2) == 'g') {
							if (i < line.length() && line.charAt(i+3) == 'h') {
								if (i < line.length() && line.charAt(i+4) == 't') {
									list.add(8);
								}
							}
						}
					}
				}
				if (i < line.length() && line.charAt(i) == 'n') {
					if (i < line.length() && line.charAt(i+1) == 'i') {
						if (i < line.length() && line.charAt(i+2) == 'n') {
							if (i < line.length() && line.charAt(i+3) == 'e') {
								list.add(9);
							}
						}
					}
				}
			}
			sum += 10*list.get(0)+list.get(list.size()-1);
		}
		System.out.println(sum);

	}

}
