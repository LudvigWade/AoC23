import java.io.File;
import java.io.FileNotFoundException;
import java.util.Scanner;

public class Day2 {
	
	public static void main(String[] args) throws FileNotFoundException {
		Scanner s = new Scanner(new File("data"));
		s.useDelimiter("(,|:|;)");
		for(int i = 0; i<10; i++) {
			System.out.println(s.next());
		}
	}
}
